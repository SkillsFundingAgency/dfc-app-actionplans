            "value": "__UserSessionsCollection__"
        },
        "ContentCollection": {
            "value": "__ContentCollection__"
        },
        "CosmosDbResourceGroup": {
            "value": "__persSharedResourceGroup__"
        },
        "cosmosDbCollectionName": {
            "value": "__cosmosDbCollectionName__"
